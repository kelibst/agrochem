    "expo-splash-screen": "~0.30.7",
    "expo-status-bar": "~2.2.3",
    "firebase": "^12.1.0",
    "nativewind": "^4.0.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-asset": "^2.1.1",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
